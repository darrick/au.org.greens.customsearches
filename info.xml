<?xml version="1.0"?>
<extension key="au.org.greens.customsearches" type="module">
  <file>customsearches</file>
  <name>AUG custom searches</name>
  <description>Searches written for AUG</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>FIXME</author>
    <email>FIXME</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://civicrm.org/extensions/aug-searches</url>
    <url desc="Documentation">https://civicrm.org/extensions/aug-searches</url>
    <url desc="Support">https://github.com/eileenmcnaughton/au.org.greens.customsearches/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-06-29</releaseDate>
  <version>1.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>This is a a port of previous custom_php files</comments>
  <civix>
    <namespace>CRM/Customsearches</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
